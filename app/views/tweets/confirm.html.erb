<h3>以下の内容で、送信する。</h3>

<%= form_with(model: @tweet, url: tweets_path, local: true) do |form| %>
  <%= form.hidden_field :content %>

    <p>ツイート内容:<%= @tweet.content %></p>
  <%= button_tag type: :submit do %>
    <%= image_tag "tweet_button.png" %>
  <% end %>

  <%= form.submit "戻る", name: 'back' %>
<% end %>
